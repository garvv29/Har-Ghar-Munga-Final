{
  "expo": {
    "name": "HarGharMunga",
    "slug": "HarGharMunga",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "privacy": "public",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "allowScreenCapture": true,
      "allowScreenRecording": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "allowBackup": true,
      "allowScreenCapture": true,
      "allowScreenRecording": true,
      "package": "com.anonymous.HarGharMunga"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1f493b7a-fc3d-481a-8fb6-f9c131d2b159"
      }
    }
  }
}
